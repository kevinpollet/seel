{
  "name": "docku",
  "version": "0.0.0",
  "author": "kevinpollet <pollet.kevin@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kevinpollet/docku#readme",
  "repository": "github.com:kevinpollet/docku",
  "bugs": "https://github.com/kevinpollet/docku/issues",
  "keywords": [
    "docker",
    "node",
    "node-js"
  ],
  "bin": {
    "docku": "bin/docku.js"
  },
  "scripts": {
    "build": "rm -rf lib && tsc -p tsconfig.json",
    "format": "prettier --write '*.{json,md}' '{src,bin}/**/*.{ts,js}'",
    "lint": "eslint 'src/**'"
  },
  "dependencies": {
    "@types/dockerode": "^2.5.20",
    "@types/node": "^8.10.51",
    "@types/tar-fs": "^1.16.1",
    "@types/tar-stream": "^1.6.1",
    "commander": "^3.0.0",
    "dockerode": "^2.5.8",
    "multimatch": "^4.0.0",
    "tar-fs": "^2.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "prettier",
      "prettier/@typescript-eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  }
}
