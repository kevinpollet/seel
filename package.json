{
  "name": "seel",
  "version": "0.0.0",
  "author": "kevinpollet <pollet.kevin@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kevinpollet/seel#readme",
  "repository": "github.com:kevinpollet/seel",
  "bugs": "https://github.com/kevinpollet/seel/issues",
  "keywords": [
    "docker",
    "container",
    "node",
    "nodejs",
    "cli"
  ],
  "bin": "bin/seel.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib && tsc -p tsconfig.build.json",
    "format": "prettier --write '*.{json,md}' '{bin,examples,src,test}/**/*.{ts,js,json,md}'",
    "format:check": "prettier --check '*.{json,md}' '{bin,examples,src,test}/**/*.{ts,js,json,md}'",
    "lint": "eslint --max-warnings 0 '{bin,examples,src,test}/**/*.{js,ts}'",
    "prepare": "npm run build",
    "release": "standard-version --sign",
    "test": "jest --preset ts-jest"
  },
  "dependencies": {
    "@types/node": "^8.10.51",
    "commander": "^3.0.0",
    "dependency-tree": "^7.0.2",
    "dockerode": "^3.0.1",
    "fast-glob": "^3.0.4",
    "normalize-package-data": "^2.5.0",
    "semver": "^6.3.0",
    "split2": "^3.1.1",
    "tar-fs": "^2.0.0"
  },
  "devDependencies": {
    "@types/dependency-tree": "^6.1.0",
    "@types/dockerode": "^2.5.20",
    "@types/jest": "^24.0.18",
    "@types/normalize-package-data": "^2.4.0",
    "@types/semver": "^6.0.1",
    "@types/split2": "^2.1.6",
    "@types/tar-fs": "^1.16.1",
    "@types/tar-stream": "^1.6.1",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.1.0",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.1",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "~3.6.3"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "parserOptions": {
          "ecmaVersion": 9
        },
        "env": {
          "node": true
        },
        "extends": [
          "eslint:recommended",
          "prettier"
        ]
      },
      {
        "files": [
          "*.ts"
        ],
        "parser": "@typescript-eslint/parser",
        "plugins": [
          "@typescript-eslint"
        ],
        "parserOptions": {
          "project": "./tsconfig.json"
        },
        "extends": [
          "eslint:recommended",
          "plugin:@typescript-eslint/eslint-recommended",
          "plugin:@typescript-eslint/recommended",
          "plugin:@typescript-eslint/recommended-requiring-type-checking",
          "prettier",
          "prettier/@typescript-eslint"
        ]
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --max-warnings 0",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  }
}
